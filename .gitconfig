[user]
	email = your.elkin@gmail.com
	name = Alexey Yanchenko
[push]
	default = current
[pull]
	default = current
[core]
	excludesfile = ~/.gitignore_global
[alias]
  br   = branch
  ci   = commit
  co   = checkout
  last = log -1 HEAD
  lg   = log --all --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
  st   = status

